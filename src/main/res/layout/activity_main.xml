<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/main_banner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="banner"/>

    <Button
        android:id="@+id/main_revealbanner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="revealbanner"/>

    <Button
        android:id="@+id/main_smartrefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="smartrefresh"/>

    <Button
        android:id="@+id/main_swiperefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="swiperefresh"/>

    <Button
        android:id="@+id/main_mpandroidchart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="mpandroidchart"/>

    <Button
        android:id="@+id/main_echart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="echart"/>

</LinearLayout>